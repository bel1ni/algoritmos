algoritmo "salario"
// Função :
// Autor :
// Data : 07/10/2024
// Seção de Declarações 
var
   nome: caractere
   salario: caractere
inicio
      Escreval ("Digite o nome do funcionário: ")
      Leia (nome)
      Escreval ("Digite o salário: ")
      Leia (salario)
      Escreva ("O funcionário ", nome, " tem um salário de R$", salario, ".")
fimalgoritmo
